type Query {
    getPayments : [Payment]
}
type Mutation {
    processPayment(prePaymentDetails: PrePaymentDetails!) : Payment!
}

input PrePaymentDetails {
    price: String
    priceModifier: Float
    paymentMethod : String
    dateTime : String
}

type Payment {
    id: ID
    finalPrice: String
    points: Int
    paymentMethod : String
    dateTime(format: String = "yyyy-MM-dd'T'HH:mm:ss'Z'") : String
}

